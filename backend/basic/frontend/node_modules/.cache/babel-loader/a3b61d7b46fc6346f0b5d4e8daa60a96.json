AA,cAAA,EACA,IADA,CACA,UAAA,QAAA;AAAA,aAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAAA,KADA;AAGA,SAAA,KAAA,CAAA,GAAA,CAAA,eAAA,EACA,IADA,CACA,UAAA,QAAA;AAAA,aAAA,KAAA,CAAA,MAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA;AAAA,KADA;AAGA,SAAA,KAAA,CAAA,GAAA,CAAA,cAAA,EACA,IADA,CACA,UAAA,QAAA;AAAA,aAAA,KAAA,CAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AAAA,KADA;AAEA,GArBA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AAAA;;AACA,UAAA,QAAA;AACA,WAAA,MAAA,CAAA,UAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AACA;AACA,YAAA,CAAA,GAAA,QAAA,CAAA,cAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAFA,CAGA;;AACA,YAAA,CAAA,CAAA,OAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,CAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,EAAA;AACA;AACA,OARA;AAUA,WAAA,KAAA,CAAA,GAAA,CAAA,qBAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,UAAA,EAAA;AAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,sBAAA,QAAA,CAAA,IAAA;AAAA,OAFA;AAIA,UAAA,OAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AACA;AACA,YAAA,CAAA,GAAA,QAAA,CAAA,cAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAFA,CAGA;;AACA,YAAA,CAAA,CAAA,OAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,EAAA;AACA;AACA,OARA;AAUA,WAAA,KAAA,CAAA,GAAA,CAAA,oBAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,SAAA,EAAA;AAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,sBAAA,QAAA,CAAA,IAAA;AAAA,OAFA;AAIA;AAhCA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"row\">\r\n    <div class=\"col-md-3\">\r\n      <div class=\"filter\">\r\n        <h5>Категория</h5>\r\n        <ul class=\"list-unstyled\">\r\n          <li v-for=\"category in filter.categories\">\r\n            <label>\r\n              <input type=\"CheckBox\" :id=\"category.name\"> {{category.name}}\r\n            </label>\r\n          </li>\r\n        </ul>\r\n        <hr>\r\n      </div>\r\n      <div class=\"filter\">\r\n        <h5>Начинка</h5>\r\n        <ul class=\"list-unstyled\">\r\n          <li v-for=\"filling in filter.fillings\">\r\n            <label>\r\n              <input type=\"checkbox\" :id=\"filling.name\"> {{filling.name}}\r\n            </label>\r\n          </li>\r\n        </ul>\r\n        <hr>\r\n      </div>\r\n      <button class=\"btn btn-secondary\" v-on:click=\"checkCategory\">Применить</button>\r\n    </div>\r\n    <div class=\"col-md-9\">\r\n      <div class=\"row\">\r\n        <div class=\"col-lg-4\" v-for=\"item in items\">\r\n          <Product v-bind:product=\"item\"></Product>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Product from \"@/components/products/Product\";\r\nexport default {\r\n  name: \"ProductList\",\r\n  components: {Product},\r\n  data() {\r\n    return {\r\n      filter: {\r\n        categories: [],\r\n        fillings: []\r\n      },\r\n      items: []\r\n    }\r\n  },\r\n  created() {\r\n    this.$http.get('/product/all')\r\n        .then((response) => this.items = response.data)\r\n\r\n    this.$http.get('/category/all')\r\n        .then((response) => this.filter.categories = response.data)\r\n\r\n    this.$http.get('/filling/all')\r\n        .then((response) => this.filter.fillings = response.data)\r\n  },\r\n  methods:{\r\n    checkCategory(){\r\n      var category;\r\n      this.filter.categories.forEach(function(item, i) {\r\n        // console.log( i + \": \" + item.name );\r\n        var x = document.getElementById(item.name);\r\n        // console.log(x);\r\n        if (x.checked) {\r\n          console.log('Выбран ' + item.name + '  ' + item.id);\r\n          category = item.id;\r\n        }\r\n      });\r\n\r\n      this.$http.get('/product/idcategory', {params: {idCategory: category}})\r\n            .then((response) => {console.log(response.data)\r\n              this.items = [...response.data]})\r\n\r\n      var filling;\r\n      this.filter.fillings.forEach(function(item, i) {\r\n        // console.log( i + \": \" + item.name );\r\n        var x = document.getElementById(item.name);\r\n        // console.log(x);\r\n        if (x.checked) {\r\n          console.log('Выбран ' + item.name + '  ' + item.id);\r\n          filling = item.id;\r\n        }\r\n      });\r\n\r\n      this.$http.get('/product/idfilling', {params: {idFilling: filling}})\r\n          .then((response) => {console.log(response.data)\r\n            this.items = [...response.data]})\r\n\r\n    }\r\n  }\r\n}\r\n\r\n</script>"],"sourceRoot":"src/components/products"}]}